{
    "name": "relay-boilerplate",
    "version": "0.1.1",
    "description": "Prime transfers client-side application",
    "packageManager": "yarn@3.1.1",
    "repository": {
        "type": "git",
        "url": "git@github.com:via-profit-services/relay-boilerplate.git"
    },
    "engines": {
        "node": ">=16.x"
    },
    "author": "Via Profit",
    "license": "MIT",
    "relay": {
        "src": "./src",
        "artifactDirectory": "./src/relay/artifacts",
        "schema": "./src/relay/schema.graphql",
        "customScalars": {
            "DateTime": "string",
            "Date": "string",
            "Time": "string",
            "EmailAddress": "string",
            "URL": "string",
            "Money": "number",
            "JSON": "any",
            "JSONObject": "any",
            "FileUpload": "File",
            "Void": "null"
        },
        "language": "typescript",
        "exclude": [
            "**/node_modules/**",
            "**/__mocks__/**",
            "**/artifacts/**"
        ]
    },
    "scripts": {
        "start": "yarn start:client:dev",
        "deploy": "sh ./scripts/deploy.sh",
        "relay": "yarn relay-compiler",
        "download-schema": "yarn node ./scripts/download-schema.js",
        "i18n:extract": "yarn i18n:extract:en && yarn i18n:extract:ru",
        "i18n:compile": "yarn formatjs compile-folder ./lang ./src/translations",
        "i18n:extract:en": "yarn formatjs extract 'src/components/**/**/*.{ts,tsx}' 'src/utils/**/**/*.ts' 'src/containers/**/**/*.{ts,tsx}' 'src/providers/**/*.{ts,tsx}' --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file lang/en.json",
        "i18n:extract:ru": "jq -s '.[0] * .[1]' ./lang/en.json ./lang/ru.json > ./lang/tmp.json && mv ./lang/tmp.json ./lang/ru.json",
        "i18n": "yarn i18n:extract && yarn i18n:compile",
        "start:server:dev": "yarn build:server:dev && node ./build/server.js",
        "start:client:dev": "yarn cross-env NODE_ENV=development webpack serve --progress --color --config ./webpack/webpack-config-dev.ts",
        "build:dist": "yarn build:prod && yarn build:server",
        "debug": "cross-env DEBUG=true yarn start:client:dev",
        "build:prod": "yarn webpack  --progress --color --config ./webpack/webpack-config-prod.ts",
        "start:prod": "yarn build:prod && yarn node ./dist/server.js",
        "build:server": "yarn webpack  --progress --color --config ./webpack/webpack-config-server.ts",
        "build:server:dev": "yarn webpack --progress --color --config ./webpack/webpack-config-server-dev.ts",
        "analyze": "yarn cross-env ANALYZE=analyze yarn build:prod",
        "lint": "yarn eslint --fix .",
        "pretty": "yarn prettier --write ./src"
    },
    "dependencies": {
        "@babel/plugin-transform-regenerator": "^7.16.7",
        "@formatjs/cli": "^4.8.2",
        "@hookform/resolvers": "^2.8.8",
        "@types/draft-js": "^0.11.9",
        "compression": "1.7.4",
        "dotenv": "^16.0.0",
        "downshift": "^6.1.7",
        "draft-js": "^0.11.7",
        "graphql-ws": "^5.5.5",
        "history": "5.2.0",
        "js-cookie": "^3.0.1",
        "jsonwebtoken": "^8.5.1",
        "libphonenumber-js": "^1.9.48",
        "mdi-react": "^8.2.0",
        "node-gyp": "^8.4.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-error-boundary": "^3.1.4",
        "react-helmet": "^6.1.0",
        "react-hook-form": "^7.26.1",
        "react-intl": "^5.24.6",
        "react-is": "^17.0.2",
        "react-redux": "^7.2.6",
        "react-relay": "^13.1.0",
        "react-router": "^6.2.1",
        "react-router-config": "^5.1.1",
        "react-router-dom": "^6.2.1",
        "react-text-mask": "^5.4.3",
        "react-window": "^1.8.6",
        "react-window-infinite-loader": "^1.0.7",
        "redux": "^4.1.2",
        "relay-runtime": "^13.1.0",
        "reselect": "^4.1.5",
        "styled-components": "^5.3.3",
        "subscriptions-transport-ws": "^0.11.0",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.17.0",
        "@babel/core": "^7.17.2",
        "@babel/plugin-proposal-class-properties": "^7.16.7",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.7",
        "@babel/plugin-proposal-optional-chaining": "^7.16.7",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.17.0",
