        "relay-compiler-language-typescript": "^15.0.1",
        "shebang-loader": "^0.0.1",
        "style-loader": "^3.3.1",
        "stylelint": "^14.4.0",
        "stylelint-config-recommended": "^7.0.0",
        "stylelint-config-styled-components": "^0.1.1",
        "stylelint-processor-styled-components": "^1.10.0",
        "svg-url-loader": "^7.1.1",
        "ts-node": "^10.5.0",
        "typescript": "^4.5.5",
